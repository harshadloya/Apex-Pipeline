package edu.bing.beans;

public class Instruction {

	String instr_type;
	String src1;
	String src2;
	String dest;
	String literal;
	
	public String getInstr_type() {
		return instr_type;
	}
	public void setInstr_type(String instr_type) {
		this.instr_type = instr_type;
	}
	public String getSrc1() {
		return src1;
	}
	public void setSrc1(String src1) {
		this.src1 = src1;
	}
	public String getSrc2() {
		return src2;
	}
	public void setSrc2(String src2) {
		this.src2 = src2;
	}
	public String getDest() {
		return dest;
	}
	public void setDest(String dest) {
		this.dest = dest;
	}
	public String getLiteral() {
		return literal;
	}
	public void setLiteral(String literal) {
		this.literal = literal;
	}
	@Override
	public String toString() {
		return "Instruction [instr_type=" + instr_type + ", src1=" + src1 + ", src2=" + src2 + ", dest=" + dest
				+ ", literal=" + literal + "]";
	}
	
	
}
